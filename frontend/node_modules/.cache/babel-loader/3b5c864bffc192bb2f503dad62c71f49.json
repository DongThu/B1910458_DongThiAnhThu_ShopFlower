{"ast":null,"code":"import axios from \"axios\";\nimport { mapState } from \"vuex\";\nexport default {\n  props: ['bill'],\n  name: \"OrderDetails\",\n\n  data() {\n    return {\n      allFlowersInBill: [],\n      item_qty: [],\n      billMatch: undefined\n    };\n  },\n\n  created() {\n    this.getAllFlowers();\n    this.getBillStatus();\n  },\n\n  computed: { ...mapState([\"allFlowers\"]),\n    filterFlowers: function () {\n      return this.allFlowers.filter(f => this.matchID(f, this.allFlowersInBill));\n    }\n  },\n  methods: {\n    matchID: function (flower, cartArray) {\n      let temp = \"\";\n      cartArray.forEach(element => {\n        if (parseInt(flower.flower_id) == element) {\n          temp = flower;\n        }\n      });\n      return temp;\n    },\n\n    async getAllFlowers() {\n      if (this.bill) {\n        let data = (await axios.get('/billdetails/' + this.bill)).data;\n        data.forEach(element => {\n          this.allFlowersInBill.push(element.flower_id);\n          this.item_qty.push(element.item_qty);\n        });\n      }\n    },\n\n    async getBillStatus() {\n      if (this.bill) {\n        this.billMatch = (await axios.get('/billstatus/bill/' + this.bill)).data[0];\n      }\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA8BA,OAAOA,KAAP,MAAkB,OAAlB;AACA,SAASC,QAAT,QAAyB,MAAzB;AACA,eAAe;EACXC,KAAK,EAAE,CAAC,MAAD,CADI;EAEXC,IAAI,EAAE,cAFK;;EAIXC,IAAI,GAAG;IACH,OAAO;MACHC,gBAAgB,EAAE,EADf;MAEHC,QAAQ,EAAE,EAFP;MAIHC,SAAS,EAAEC;IAJR,CAAP;EAMH,CAXU;;EAaXC,OAAO,GAAG;IACN,KAAKC,aAAL;IACA,KAAKC,aAAL;EACH,CAhBU;;EAkBXC,QAAQ,EAAE,EACN,GAAGX,QAAQ,CAAC,CAAC,YAAD,CAAD,CADL;IAGNY,aAAa,EAAE,YAAY;MACvB,OAAO,KAAKC,UAAL,CAAgBC,MAAhB,CACFC,CAAD,IAAO,KAAKC,OAAL,CAAaD,CAAb,EAAgB,KAAKX,gBAArB,CADJ,CAAP;IAGH;EAPK,CAlBC;EA4BXa,OAAO,EAAE;IACLD,OAAO,EAAE,UAAUE,MAAV,EAAkBC,SAAlB,EAA6B;MAClC,IAAIC,IAAG,GAAI,EAAX;MACAD,SAAS,CAACE,OAAV,CAAkBC,OAAM,IAAK;QACzB,IAAIC,QAAQ,CAACL,MAAM,CAACM,SAAR,CAAR,IAA8BF,OAAlC,EAA2C;UACvCF,IAAG,GAAIF,MAAP;QACJ;MACH,CAJD;MAKA,OAAO